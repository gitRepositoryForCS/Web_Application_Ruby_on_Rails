<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th> Title </th>
			<th> Description </th>
			<th> Date </th>
			<th> Location </th>
			<th> Status </th>
			<th> Created By </th>
			<th> Host </th>
			<th> Event Type </th>
		</tr>
	</thead>
	<% @events.each do |event| %>
		<tr>
			<td> <%= event.title %> </td>
			<td> <%= event.description %> </td>
			<td> <%= event.date %> </td>
			<td> <%= event.location %> </td>
			<td> <%= event.approved %> </td>
		
			<td> <%= event.host %> </td>
			<td> <%= event.event_type_id %> </td>
		</tr>
	<% end %>
</table>
